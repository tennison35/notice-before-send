{
   "name": "Notice Before Send",
   "version": "0.0.1",
   "description": "Notice before you send email to external parties in Gmail :)",
   "manifest_version": 2,
   "content_scripts": [ {
      "js": [ "js/jquery.js", "js/app.js" ],
      "css": [ "css/notice.css" ],
      "matches": [ "http://mail.google.com/*", "https://mail.google.com/*" ],
      "run_at": "document_end"
   } ],
   "icons": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
   },
   "permissions": [ "*://mail.google.com/" ],
   "web_accessible_resources": [ "js/jquery.js", "js/app.js" ]
}
